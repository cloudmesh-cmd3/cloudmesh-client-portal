{% extends 'cloudmesh_portal/layout/index.jinja' %}

{% block content %}

<h2>Parameters</h2>


<table  id="dictTable" class="table table-striped table-bordered">
    <tbody>
        {% for key in parameters %}
            <tr>
                <td>
                    {{ key }}
                </td>
                <td>
                    {{ parameters[key] }}
                </td>
            </tr>
        {% endfor %}
    </tbody>
</table>

<h2>Response</h2>

{%if response %}
    <div name="response" style="font-weight:bold;white-space:pre-wrap;">
        <pre>
        {{ response }}
        </pre>
    </div>
{% endif %}


{% endblock %}
