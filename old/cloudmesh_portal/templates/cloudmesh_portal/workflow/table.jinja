{% extends 'cloudmesh_portal/layout/index.jinja' %}

{% block title %}
{{ title }}
{% endblock %}

{% block content %}


<script type="text/javascript" charset="utf-8">
    $(document).ready(function() {
        $('#dictTable').DataTable({
           aLengthMenu: [
                [10, 20, 50, 100, 200, -1],
                [10, 20, 50, 100, 200, "All"]
            ],
            iDisplayLength: 10,
            responsive: true,
        });
    } );
</script>

<table  id="dictTable" class="table table-striped table-bordered">
    <thead>
        <tr>
            <th> Name </th>
            <th> Filename </th>
            <th> Detail </th>
            <th> Graph </th>
        </tr>
    </thead>
    <tbody>
        {% for element in data %}
        <tr>
            <td> {{ data[element]['name'] }} </td>
            <td> {{ data[element]['filename'] }} </td>
            <td><a
                    href="/cm/workflow/detail/{{ data[element]['name'] }}/">detail
            </a></td>
            <td><a href="/cm/workflow/graph/{{ data[element]['name'] }}/">graph</a></td>
        </tr>
        {% endfor %}
    </tbody>
</table>



{% endblock %}
